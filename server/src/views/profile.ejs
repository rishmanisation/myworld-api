<head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon" href="../../../../favicon.ico">
    
        <title>Your profile</title>

    
        <!-- Bootstrap core CSS -->
        <link href="bootstrap.min.css" rel="stylesheet">
    
        <!-- Custom styles for this template -->
        <link href="navbar-top.css" rel="stylesheet">
      </head>
    
      <body>
    
        <nav class="navbar navbar-expand-md navbar-dark bg-dark mb-4">
          <a class="navbar-brand" href="/api/profile">Authenticator Application</a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarCollapse">
            <ul class="navbar-nav mr-auto">
              <li class="nav-item active">
                <a class="nav-link" href="/api/profile">Home<span class="sr-only">(current)</span></a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/api/whitelist">Whitelist</a>
              </li>
            </ul>
            <ul>
                <li class="nav-item mt-2 mt-md-0">
                    <a class="nav-link" href="/api/logout">Logout</a>
                </li>                
            </ul>
          </div>
        </nav>
    
        <main role="main" class="container">
          <div class="jumbotron">
            <h1>Hello, <%= user.first_name %>!</h1>
            <p class="lead"><strong>Your information: <br/></strong>
                User ID: <%= user.user_id %><br/>
                Full Name: <%= user.first_name + ' ' + user.last_name %><br/>
                Email Address: <%= user.email_id %><br/>
            </p>
            <a class="btn btn-lg btn-primary" href="/api/whitelist" role="button">Whitelist another user &raquo;</a>
          </div>

          <div class="jumbotron">
            <h1>Whitelisted Users</h1>
          <div class="container">
              <div class="row">
                <% for (var i = 0; i < allUsers.length; i++) {%>
                  <% if (i % 3 == 0) {%>
                  </div><div class="row">
                  <%} %>
                    <div class="col-md-4">
                        <div class="card mb-4 shadow-sm">
                        <div class="card-body">
                            <p class="card-text"><%= allUsers[i].name %></p>
                            <p class="card-text"><%= allUsers[i].email_id %></p>
                            <div class="d-flex justify-content-between align-items-center"></div>
                        </div>
                        </div>
                    </div>
                <%} %>
            </div>
            </div>
        </main>
    
        <!-- Bootstrap core JavaScript
        ================================================== -->
        <!-- Placed at the end of the document so the pages load faster -->
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
        <script src="bootstrap.min.js"></script>
      </body>

